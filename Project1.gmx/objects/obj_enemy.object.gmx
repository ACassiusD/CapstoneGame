}

//Move Horizontally
x += hspd;


//Vertical Collisions
if (place_meeting(x, y+vspd, oParSolid))
{
    while (!place_meeting(x, y+sign(vspd), oParSolid))
    {
        y += sign(vspd);
    }
    vspd = 0;
    
    if (fearOfHeights) &amp;&amp; !position_meeting(x+(sprite_width/2)*dir, y+(sprite_height/2)+8, oParSolid)
    {
        dir *= -1;
    }
}

//Move Vertically
y += vspd;

//Enemy Collision
if (place_meeting(x,y,oPlayerAtkBox))
{
    instance_destroy();
}

if (place_meeting(x,y,oPlayer))
{
    room_restart();
}


</string>
